@page "/DocsIn/List"

@attribute [Authorize]

@using Pie.Pages.DocsIn.Components

<SearchForm Warehouses="@warehouses" OnSubmit="SearchHandle"></SearchForm>

<SearchByStatusTabs Statuses="@statuses" CountByStatus="@countByStatus" OnClick="SearchHandle"></SearchByStatusTabs>

@if (docs is not null && docs.Any())
{
    <CascadingValue Value="@SearchParameters">
        <CardsQueues Docs="@docs" Queues="@queues"></CardsQueues>
    </CascadingValue>
}
